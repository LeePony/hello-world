<div class="row">
	<div class="col-sm-12 col-md-12">
		<div class="box">
			<div class="box-header">
				<h3 class="box-title">{{SC_TRANSFER}}</h3>
			</div>
			<div class="box-con">
				<section class="page-section">
					<div id="tabs" class="ui-tabs">
						<ul class="tabs-nav-list">
                            <li>
                                <a href="#tabs-a">{{SC_TRANSFER}}</a>
                            </li>
                            <li>
                                <a id="transferHisTab" href="#tabs-b">{{SC_HISTORY}}</a>
                            </li>
                        </ul>
                        <div id="tabs-a" class="ui-tabs-panel">
                        	<div class="description">
								<div><i class="iconfont icon-tishi"></i></div>
								<ul>
									<li><span> * </span>{{SC_TRANSFER_BAL_COMMENTS1}}</li>
									<li><span> * </span>{{SC_TRANSFER_BAL_COMMENTS2}}</li>
									<li><span> * </span>{{SC_TRANSFER_BAL_COMMENTS3}}</li>
									<li><span> * </span>{{SC_TRANSFER_BAL_COMMENTS4}}</li>
								</ul>
							</div>
							<div class="col-xs-12 col-sm-12 col-md-12">
								<form id="transferForm" class="form form-horizontal">
									<div class="form-group">
										<label class="col-sm-4 control-label">{{SC_MOBILE_NUMBER}}</label>
										<div class="col-sm-4 input-group">
											<input id="accNbrId" type="text" class="form-control form-control-static" readonly value="">
										</div>
									</div>
									<div class="form-group" id="myBalance">
										<label class="col-sm-4 control-label">{{SC_MY_BALANCE}}</label>
										<div class="col-sm-4 input-group">
										  	<input id="myBalanceId" type="text" class="form-control form-control-static lost-active" readonly value="">
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-4 control-label">{{SC_RECEIVER_NUMBER}}</label>
										<div class="col-xs-10 col-sm-5 input-group form-help-box transfer">
										  	<input id="recNbr" type="text" class="form-control" placeholder="{{SC_ENTER_NUM}}" data-rule="required; digits;">
                              				<span class="form-help" id="historyNum"><i class="iconfont icon-jingcailishijilu"></i></span>
											<div id="hisNbrDiv" class="history-num-list">
												
											</div>
										</div>
									</div>
									<div class="form-group">
										<label class="col-sm-4 control-label">{{SC_TRANSFER_AMOUNT}}</label>
										<div id="chargeBtnDiv" class="col-sm-8 input-group"></div>
									</div>
									<div class="form-group" style="display:none">
										<label class="col-sm-4 control-label">{{SC_SERVICE_CHARGE}}</label>
										<div class="col-sm-4 input-group">
										<input type="text" class="form-control form-control-static num static-inline" readonly value="" style="width:100px;"><span class="other-info">(x%)</span>
										</div>
									</div>
									<div class="form-group form-button">
										<div class="col-sm-offset-4 col-sm-4 input-group">
										  <button id="transferBtn" type="button" class="btn btn-primary">{{SC_SUBMIT}}</button>
										</div>
									</div>
								</form>
							</div>
                        </div>
                        <div id="tabs-b" class="ui-tabs-panel">
                        	<div class="description">
								<div><i class="iconfont icon-tishi"></i></div>
								<ul>
									<li><span> * </span>{{SC_TRANSFER_HIS_COMMENTS1}}</li>
									<li><span> * </span>{{SC_TRANSFER_HIS_COMMENTS2}}</li>
									<li><span> * </span>{{SC_TRANSFER_HIS_COMMENTS3}}</li>
								</ul>
							</div>
							<div class="row history-date-list">
								<div class="col-xs-12 col-sm-12 col-md-12">
									<button id="threeMonth" type="button" class="btn btn-date">{{SC_NEARLY_THREE_MONTH}}</button>
									<button id="sixMonth" type="button" class="btn btn-date">{{SC_NEARLY_SIX_MONTH}}</button>
									<button type="button" id="customTime" class="btn btn-date clearfix">{{SC_CUST_TIME}}</button>
									<form id="searchForm">
									<div class="custom-time">
		                                <div class="col-xs-12 col-sm-5 no-padding">
		                                	<div class="input-group"><input type="text" id="startDate" class="form-control" data-rule="required;date;"></div>
		                                </div>
		                                <div class="col-xs-12 col-sm-5 no-padding">
		                                	<div class="input-group"><input type="text" id="endDate" class="form-control" data-rule="required;match[gte, startDate, date];"></div>
		                                </div>
										<button id="searchBtn" type="button" class="search"><i class="fa fa-search" aria-hidden="true"></i></button>
									</div>
									</form>
								</div>
							</div>
							<div class="table-box">
								<div id="transferHisGrid"></div>
							</div>
                        </div>
                    </div>
				</section>
			</div>
		</div>
	</div>
</div>