@charset "UTF-8";
/*  样式是由 Sass 编译生成的，不要直接修改 .css 文件  */
.bg-red {
  background-color: #db261d !important;
}

.bg-blue {
  background-color: #024ea2 !important;
}

.bg-white {
  background-color: #fff !important;
}

.bg-gray {
  background-color: transparent !important;
}

.shadow {
  font-weight: 600;
}

.blue-color {
  color: #024ea2;
}

::-webkit-scrollbar {
  width: 12px;
  height: 12px;
}

::-webkit-scrollbar-track {
  background-color: #F0F0F0;
}

::-webkit-scrollbar-thumb {
  border: 2px solid transparent;
  border-radius: 10px;
  background-color: rgba(50, 50, 50, 0.25);
  background-clip: padding-box;
}

::-webkit-scrollbar-thumb:hover {
  background-color: rgba(50, 50, 50, 0.5);
}

html, body {
  width: 100%;
  height: 100%;
  margin: 0;
  font-family: "Open Sans", "Microsoft YaHei", "微软雅黑", Arial, sans-serif;
}

body {
  font-size: 14px;
  background-color: #efefef;
}

.webpage-wrapper {
  width: 100%;
  min-height: 100%;
  max-width: 1680px;
  min-width: 308px;
  background-color: #efefef;
  margin: 0 auto;
  position: relative;
}
.webpage-header {
  width: 100%;
  height: 98px;
}
.webpage-ad {
  width: 100%;
  height: auto;
  text-align: center;
  margin-bottom: 20px;
}
.webpage-ad img {
  width: 100%;
}
@media screen and (max-width: 767px) {
  .webpage-ad {
    margin-bottom: 10px;
  }
}
.webpage-content {
  max-width: 1200px;
  width: 100%;
  margin: 0 auto;
  overflow-y: hidden;
  margin-bottom: 20px;
}
@media screen and (max-width: 1200px) {
  .webpage-content {
    width: auto;
    margin: 0 20px;
  }
}
@media screen and (max-width: 767px) {
  .webpage-content {
    margin: 0 10px;
  }
}
.webpage-content-inner {
  min-height: 100%;
  padding-top: 0;
  background-color: #fff;
}
.webpage-footer {
  height: 36px;
}
@media screen and (max-width: 1024px) {
  .webpage-wrapper .bottom-fill {
    display: none;
    position: absolute;
    top: 98px;
    width: 100%;
    height: 100%;
    z-index: 9;
  }
  .webpage-wrapper .bottom-fill.show {
    display: block;
  }
  .webpage-wrapper .bottom-fill .overlay {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.2);
  }
}

.webpage-content-inner {
  display: table;
  width: 100%;
}
@media (max-width: 767px) {
  .webpage-content-inner {
    display: block;
  }
}
.content-aside {
  display: table-cell;
  float: left;
  width: 330px;
  background-color: #fff;
  -webkit-transition: width .35s,visibility .35s;
          transition: width .35s,visibility .35s;
  vertical-align: top;
}
@media screen and (max-width: 1200px) {
  .content-aside {
    display: none;
    position: absolute;
    z-index: 99;
    top: 98px;
    right: 0;
    box-shadow: -3px 3px 10px 0px rgba(0, 0, 0, 0.3);
  }
  .content-aside.close-aside {
    display: none;
  }
  .content-aside.show-aside {
    display: block;
  }
}
.content-main {
  display: table-cell;
  height: 100%;
  overflow: hidden;
  border-left: 15px solid #efefef;
  width: 885px;
  vertical-align: top;
}
@media screen and (max-width: 1200px) {
  .content-main {
    width: 100%;
    border: none;
    display: block;
  }
}
@media screen and (max-width: 768px) {
  .content-main {
    font-size: .875em;
  }
}

